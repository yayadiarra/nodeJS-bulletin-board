

pipeline {
agent any
  tools {
    maven "maven-nara"
  }
  environment {
        
       sh = "${nodejs-bulletin-board/jenkins}"
   
        }
  stages {
    
    stage ('Maven Clean'){
      steps{
        sh 'mvn clean'
      }
    }
    stage ('Maven package') { 
      steps{
        sh 'mvn package'
      }
   }
