
pipeline {
  agent any

  stages {
 
    stage('docker image') {
      
      steps {
       
       withDockerRegistry([ credentialsId: "Docker_hub", url: "https://index.docker.io/v1/" ]) {
       sh 'docker build -t devopstrainingschool/java-nodejs-jenkins . -f Dockerfile'
       sh 'docker push devopstrainingschool/java-nodejs-jenkins'
        
        }
       
        
     
         
      }
     }
      }
      }
